    attrs
        host = "content"
        style = "upload"
        owner = "api-platform"
        feature = "upload_api_rate_limit"
        allow_app_folder_app = true
        select_admin_enabled = true


struct UploadSessionCursor
    session_id String
        "The upload session ID (returned by :route:`upload_session/start`)."
    offset UInt64
