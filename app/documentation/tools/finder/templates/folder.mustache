<div class="window {{#classes}}{{class}} {{/classes}}">

  <div class="toolbar">
    <div class="dots">
      <div class="dot"></div>
      <div class="dot"></div>
      <div class="dot"></div>
    </div>
    <span class="icon dropbox"></span>&nbsp;{{title}}
  </div>

  <div class="sorter">Name <span class="icon chevron"></span></div>

  <div class="contents">
    {{#contents}}
    <div class="item {{#nested}}{{nested}}-{{depth}}{{/nested}} {{open}}">{{#name}}<span class="icon {{folder}} {{file}} {{extension}}"></span> {{name}}{{/name}}{{^name}}&nbsp;{{/name}}</div>
    {{/contents}}
  </div>
  
  <div class="border left"></div>
  <div class="border right"></div>
  <div class="border top"></div>
  <div class="border bottom"></div>

  <div class="corner top left"></div>
  <div class="corner top right"></div>
  <div class="corner bottom left"></div>
  <div class="corner bottom right"></div>
</div>