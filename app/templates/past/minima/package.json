{
  "name": "Minima",
  "locals": {
    "author": "GitHub User",
    "email": "your-email@domain.com",
    "description": "Write an awesome description for your new site here. You can edit this line in _config.yml. It will appear in your document head meta (for Google search results) and in your feed.xml site description.\n",
    "show_excerpts": false,
    "minima": {
      "social_links": {
        "twitter": "jekyllrb",
        "github": "jekyll",
        "rss": "rss"
      }
    }
  },
  "views": {
    "minima-social-icons": {
      "url": "/minima-social-icons.svg",
      "partials": {}
    },
    "archives": {
      "url": "/archives",
      "partials": {
        "title": "Archives - {{{title}}}",
        "description": "All the entries posted on {{{title}}}"
      }
    },
    "tagged": {
      "partials": {
        "title": "Entries tagged {{{tag}}} from {{{title}}}",
        "description": "All of the entries posted on {{{title}}} tagged {{{tag}}}"
      }
    },
    "entries": {
      "partials": {
        "title": "{{{title}}}",
        "description": "The latest entries posted on {{{title}}}"
      }
    },
    "error": {
      "partials": {
        "title": "{{{error.title}}} - {{{title}}}",
        "description": "Error on {{{title}}}"
      }
    },
    "entry": {
      "partials": {
        "title": "{{{entry.title}}} - {{{title}}}",
        "description": "{{{entry.summary}}}"
      }
    },
    "public": {
      "partials": {
        "title": "Index of {{{path}}}",
        "description": "All the files and folders inside {{{path}}}"
      }
    },
    "search": {
      "partials": {
        "title": "Search - {{{title}}}",
        "description": "Search all the entries posted on {{{title}}}"
      }
    },
    "style": {
      "url": "/style.css"
    },
    "script": {
      "url": "/script.js"
    },
    "sitemap": {
      "url": "/sitemap.xml"
    },
    "feed": {
      "url": "/feed.rss",
      "type": "application/xml"
    },
    "robots": {
      "url": "/robots.txt"
    }
  }
}