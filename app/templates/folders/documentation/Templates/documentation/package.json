{
  "name": "Documentation",
  "locals": {
    "title": "Documentation",
    "call_to_action": "/tickets",
    "page_size": 18,
    "sort_by": "id",
    "spacing": 1,
    "dark_slides_background": true,
    "background_color": "#fff",
    "text_color": "#000",
    "link_color": "#000",
    "dark_background_color": "#111",
    "dark_text_color": "#eee",
    "dark_link_color": "#eee",
    "hide_dates": false,
    "date_display": "MMM D",
    "syntax_highlighter": {
      "id": "github"
    },
    "dark_syntax_highlighter": {
      "id": "github-dark"
    },
    "syntax_highlighter_font": {
      "id": "plex-mono"
    },
    "body_font": {
      "id": "system-sans"
    }
  },
  "enabled": true,
  "views": {
    "error.html": {
      "partials": {
        "title": "{{{error.title}}} - {{{title}}}"
      }
    },
    "entry.html": {
      "partials": {
        "title": "{{{entry.title}}}"
      }
    },
    "entries.html": {
      "partials": {
        "title": "{{{title}}}"
      }
    }
  }
}
