{
  "name": "Mirror of photo",
  "locals": {
    "background_color": "#FFFFFF",
    "text_color": "#040817D9",
    "link_color": "#00008B",
    "hide_dates": false,
    "infinite_scroll": true,
    "sticky_navigation": true,
    "collapse_navigation_by_default": false,
    "navigation_alignment": "justified",
    "navigation_alignment_options": [
      "justified",
      "left",
      "right",
      "center"
    ],
    "thumbnail_size": "medium",
    "thumbnail_size_options": [
      "small",
      "medium",
      "large",
      "huge"
    ],
    "navigation_position": "top",
    "navigation_position_options": [
      "top",
      "bottom"
    ],
    "page_size": 42,
    "syntax_highlighter": {
      "id": "stackoverflow-light",
      "styles": ".hljs{color:#2f3337;background:#f6f6f6}.hljs-subst{color:#2f3337}.hljs-comment{color:#656e77}.hljs-attr,.hljs-doctag,.hljs-keyword,.hljs-meta .hljs-keyword,.hljs-section,.hljs-selector-tag{color:#015692}.hljs-attribute{color:#803378}.hljs-name,.hljs-number,.hljs-quote,.hljs-selector-id,.hljs-template-tag,.hljs-type{color:#b75501}.hljs-selector-class{color:#015692}.hljs-link,.hljs-regexp,.hljs-selector-attr,.hljs-string,.hljs-symbol,.hljs-template-variable,.hljs-variable{color:#54790d}.hljs-meta,.hljs-selector-pseudo{color:#015692}.hljs-built_in,.hljs-literal,.hljs-title{color:#b75501}.hljs-bullet,.hljs-code{color:#535a60}.hljs-meta .hljs-string{color:#54790d}.hljs-deletion{color:#c02d2e}.hljs-addition{color:#2f6f44}.hljs-emphasis{font-style:italic}.hljs-strong{font-weight:700}pre code.hljs{display:block;overflow:auto;padding:1em;margin:1em 0;white-space:pre-wrap;white-space:-moz-pre-wrap;white-space:-pre-wrap;white-space:-o-pre-wrap;word-wrap:break-word}.hljs,code.hljs{padding:0;margin:0}"
    },
    "date_display": "MMMM D, Y",
    "body_font": {
      "name": "Nanum Myeongjo",
      "id": "nanum-myeongjo",
      "stack": "'Nanum Myeongjo'",
      "tags": [
        "sans"
      ],
      "line_height": 1.4,
      "line_width": 38,
      "font_size": "27",
      "styles": "@font-face{font-family:'Nanum Myeongjo';font-style:normal;font-weight:400;src:url(https://cdn.blot.development/fonts/nanum-myeongjo/400.eot?version=24a94b&extension=.eot);src:url(https://cdn.blot.development/fonts/nanum-myeongjo/400.eot?version=24a94b&extension=.eot#iefix) format('embedded-opentype'),url(https://cdn.blot.development/fonts/nanum-myeongjo/400.woff?version=860315&extension=.woff) format('woff'),url(https://cdn.blot.development/fonts/nanum-myeongjo/400.ttf?version=efdc1f&extension=.ttf) format('truetype'),url(https://cdn.blot.development/fonts/nanum-myeongjo/400.otf?version=24a5e0&extension=.otf) format('opentype')}@font-face{font-family:'Nanum Myeongjo';font-style:normal;font-weight:700;src:url(https://cdn.blot.development/fonts/nanum-myeongjo/700.eot?version=dc2c1c&extension=.eot);src:url(https://cdn.blot.development/fonts/nanum-myeongjo/700.eot?version=dc2c1c&extension=.eot#iefix) format('embedded-opentype'),url(https://cdn.blot.development/fonts/nanum-myeongjo/700.woff?version=178844&extension=.woff) format('woff'),url(https://cdn.blot.development/fonts/nanum-myeongjo/700.ttf?version=5ea37d&extension=.ttf) format('truetype'),url(https://cdn.blot.development/fonts/nanum-myeongjo/700.otf?version=27e6eb&extension=.otf) format('opentype')}@font-face{font-family:'Nanum Myeongjo';font-style:normal;font-weight:800;src:url(https://cdn.blot.development/fonts/nanum-myeongjo/800.eot?version=9dff71&extension=.eot);src:url(https://cdn.blot.development/fonts/nanum-myeongjo/800.eot?version=9dff71&extension=.eot#iefix) format('embedded-opentype'),url(https://cdn.blot.development/fonts/nanum-myeongjo/800.woff?version=5959f5&extension=.woff) format('woff'),url(https://cdn.blot.development/fonts/nanum-myeongjo/800.ttf?version=bf37d9&extension=.ttf) format('truetype'),url(https://cdn.blot.development/fonts/nanum-myeongjo/800.otf?version=a2d814&extension=.otf) format('opentype')}.pull-double{margin-left:-.3203125em}.push-double{margin-right:.3203125em}.pull-single{margin-left:-.17578125em}.push-single{margin-right:.17578125em}.push-double,.push-single{display:inline-block}"
    },
    "spacing": "1",
    "spacing_range": [
      0,
      5
    ],
    "grid_layout": "square",
    "grid_layout_options": [
      "square",
      "bookshelf"
    ],
    "demo_folder": "william"
  },
  "enabled": true,
  "views": {
    "entry.html": {
      "partials": {
        "title": "{{{entry.title}}} - {{{title}}}",
        "description": "{{{entry.summary}}}"
      }
    },
    "tagged.html": {
      "partials": {
        "title": "{{{tag}}} - {{{title}}}",
        "description": "All of the entries posted on {{{title}}} tagged {{{tag}}}"
      }
    },
    "entries.html": {
      "partials": {
        "title": "{{{title}}}",
        "description": "The latest entries posted on {{{title}}}"
      }
    },
    "archives.html": {
      "url": "/archives",
      "partials": {
        "title": "Archives - {{{title}}}",
        "description": "All the entries posted on {{{title}}}"
      }
    },
    "README": {
      "url": "/readme"
    }
  }
}
