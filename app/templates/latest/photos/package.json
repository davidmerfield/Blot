{
  "name": "Photos",
  "isPublic": true,
  "locals": {
    "page_size": 16,
    "background_color": "rgba(255, 255, 255, 1)",
    "text_color": "rgba(0, 0, 0, 0.85)",
    "font_size": 1.9,
    "line_height": 1.4,
    "spacing": 3
  },
  "views": {
    "archives": {
      "url": "/archives",
      "partials": {
        "title": "Archives - {{{title}}}",
        "description": "All the entries posted on {{{title}}}"
      }
    },
    "tagged": {
      "partials": {
        "title": "Entries tagged {{{tag}}} from {{{title}}}",
        "description":
          "All of the entries posted on {{{title}}} tagged {{{tag}}}"
      }
    },
    "entries": {
      "partials": {
        "title": "{{{title}}}",
        "description": "The latest entries posted on {{{title}}}"
      }
    },
    "error": {
      "partials": {
        "title": "{{{error.title}}} - {{{title}}}",
        "description": "Error on {{{title}}}"
      }
    },
    "entry": {
      "partials": {
        "title": "{{{entry.title}}} - {{{title}}}",
        "description": "{{{entry.summary}}}"
      }
    },
    "public": {
      "partials": {
        "title": "Index of {{{path}}}",
        "description": "All the files and folders inside {{{path}}}"
      }
    },
    "search": {
      "partials": {
        "title": "Search - {{{title}}}",
        "description": "Search all the entries posted on {{{title}}}"
      }
    },
    "style": {
      "url": "/style.css"
    },
    "script": {
      "url": "/script.js"
    },
    "sitemap": {
      "url": "/sitemap.xml"
    },
    "feed": {
      "url": "/feed.rss",
      "type": "application/xml"
    },
    "robots": {
      "url": "/robots.txt"
    }
  }
}
