{
  "name": "Reference",
  "locals": {
    "page_size": 24,
    "background_color": "#fff",
    "text_color": "#000",
    "hide_dates": false,
    "date_display": "MMMM D, Y",
    "body_font": {},
    "syntax_highlighter": {
      "id": "stackoverflow-light"
    }
  },
  "isPublic": true,
  "views": {
    "archives.html": {
      "url": "/archives",
      "partials": {
        "title": "Archives - {{{title}}}",
        "description": "Everything posted on {{{title}}}"
      }
    },
    "tags.html": {
      "url": "/tags",
      "partials": {
        "title": "Tags - {{{title}}}",
        "description": "All the tags used on {{{title}}}"
      }
    },
    "tagged.html": {
      "partials": {
        "title": "{{{tag}}} - {{{title}}}",
        "description": "{{{tag}}} on {{{title}}}"
      }
    },
    "entries.html": {
      "partials": {
        "title": "{{{title}}}",
        "description": "The latest on {{{title}}}"
      }
    },
    "error.html": {
      "partials": {
        "title": "{{{error.title}}} - {{{title}}}",
        "description": "Error on {{{title}}}"
      }
    },
    "entry.html": {
      "partials": {
        "title": "{{{entry.title}}} - {{{title}}}",
        "description": "{{{entry.summary}}}"
      }
    },
    "search.html": {
      "partials": {
        "title": "Search - {{{title}}}",
        "description": "Search everything on {{{title}}}"
      }
    }
  }
}
