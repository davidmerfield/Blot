{
  "name": "Blog",
  "locals": {
    "page_size": 6,
    "text_color": "#232323",
    "background_color": "#fff",
    "accent_color": "#004984",
    "hide_dates": false,
    "date_display": "MMMM D, Y",
    "body_font": {},
    "navigation": {
      "position": "top",
      "alignment": "spread"
    },
    "color_schemes": {
      "light": {
        "text_color": "#232323",
        "background_color": "#fff",
        "accent_color": "#004984"
      },
      "dark": {
        "text_color": "white",
        "background_color": "black",
        "accent_color": "orange"
      },
      "red": {
        "text_color": "red",
        "background_color": "pink",
        "accent_color": "purple"
      },
      "green": {
        "text_color": "green",
        "background_color": "white",
        "accent_color": "blue"
      },
      "blue": {
        "text_color": "MidnightBlue",
        "background_color": "LightCyan",
        "accent_color": "Blue"
      }
    }
  },
  "isPublic": true,
  "views": {
    "archives.html": {
      "url": "/archives",
      "partials": {
        "title": "Archives - {{{title}}}",
        "description": "All the entries posted on {{{title}}}"
      }
    },
    "tagged.html": {
      "partials": {
        "title": "{{{tag}}} - {{{title}}}",
        "description": "All of the entries posted on {{{title}}} tagged {{{tag}}}"
      }
    },
    "entries.html": {
      "partials": {
        "title": "{{{title}}}",
        "description": "The latest entries posted on {{{title}}}"
      }
    },
    "error.html": {
      "partials": {
        "title": "{{{error.title}}} - {{{title}}}",
        "description": "Error on {{{title}}}"
      }
    },
    "entry.html": {
      "partials": {
        "title": "{{{entry.title}}} - {{{title}}}",
        "description": "{{{entry.summary}}}"
      }
    },
    "search.html": {
      "partials": {
        "title": "Search - {{{title}}}",
        "description": "Search all the entries posted on {{{title}}}"
      }
    }
  }
}
